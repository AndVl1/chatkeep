# Mini App Environment Variables
# Copy this file to .env.development or .env.production

# API Base URL
# For development: /api/v1/miniapp (proxied via Vite)
# For production: https://api.chatkeep.ru/api/v1/miniapp
VITE_API_URL=/api/v1/miniapp

# Telegram Bot Username
# Used for the Telegram Login Widget in web mode
# Get this from @BotFather
VITE_BOT_USERNAME=chatkeep_bot
